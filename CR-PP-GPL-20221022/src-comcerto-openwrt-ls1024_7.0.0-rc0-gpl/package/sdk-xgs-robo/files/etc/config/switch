	option name 'lan3'
	option status 'enable'
	option eee 'enable'
	option flow_control 'enable'
	option mode 'auto'

config port 'lan4'
	option name 'lan4'
	option status 'enable'
	option eee 'enable'
	option flow_control 'enable'
	option mode 'auto'

config switch_vlan 'vlan1'
	option vlan '1'
	option routing 'enable'
	option ports '1 2 3 4'

config switch_vlan 'vlan20'
	option vlan '20'
	option routing 'enable'

config switch_vlan 'vlan100'
	option vlan '100'
	option routing 'enable'
	option ports '1t 2t 3t 4t'

